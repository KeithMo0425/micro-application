{
  "name": "ych-cashier",
  "version": "0.1.0",
  "description": "a application",
  "author": "YCH-FE",
  "main": "dist_electron/bundled/background.js",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:electron": "vue-cli-service build:electron",
    "serve:electron": "vue-cli-service serve:electron",
    "build:api": "vue-cli-service nei-api-get",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style",
    "commit": "git add . && node_modules/commitizen/bin/git-cz",
    "commit:custom": "node_modules/commitizen/bin/git-cz",
    "postinstall": "electron-builder install-app-deps",
    "changelog": "node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dayjs": "^1.7.5",
    "element-ui": "^2.4.6",
    "ffi": "^2.2.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.0",
    "@vue/cli-plugin-babel": "3.0.1",
    "@vue/cli-plugin-eslint": "3.0.1",
    "@vue/cli-service": "3.0.1",
    "@vue/eslint-config-airbnb": "3.0.1",
    "commitizen": "^2.10.1",
    "conventional-changelog-cli": "^2.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "electron": "2.0.7",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "stylelint-config-standard": "^18.2.0",
    "vue-cli-plugin-electron-builder": "1.0.0-rc.1",
    "vue-cli-plugin-element": "^1.0.0",
    "vue-cli-plugin-yuntai-project-helper": "^0.1.6",
    "vue-template-compiler": "^2.5.17",
    "yorkie": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "yarn lint && echo 'lint pass'",
    "commit-msg": "node git-hooks/verify-commit-msg.js"
  }
}
